generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int  @id @default(autoincrement())
         
}

model Friend {
  id        Int  @id @default(autoincrement())

}

model Group {
  id        Int  @id @default(autoincrement())

}

model Message {
  id        Int  @id @default(autoincrement())

}
